{
	"buildCommand": "pnpm install && pnpm run build:vercel",
	"installCommand": "pnpm install",
	"framework": "sveltekit",
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node@3.0.0"
		}
	],
	"routes": [{ "src": "/(.*)", "dest": "/" }],
	"env": {
		"DISABLE_TURTLE_DB": "true",
		"NODE_VERSION": "20.19.2"
	},
	"engines": {
		"node": "20.x"
	}
}
